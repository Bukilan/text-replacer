{
    "name": "text-replacer",
    "displayName": "replaceText",
    "description": "replace ru layout to en",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.replaceText"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
                "command": "extension.replaceText",
                "title": "Replace ru layout to en"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^10.2.0",
        "tslint": "^5.20.1",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    }
}